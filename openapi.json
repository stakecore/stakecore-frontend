{
    "openapi": "3.0.0",
    "paths": {
        "/api/form/submit": {
            "post": {
                "operationId": "FormController_submitForm",
                "summary": "Submit form data",
                "parameters": [],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/FormDto"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Successful form submission",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiResponseDto"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Form"
                ]
            }
        },
        "/api/form/messages": {
            "get": {
                "operationId": "FormController_getMessages",
                "summary": "Get all messages",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "Fetched all messages",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiResponseDto"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Form"
                ],
                "security": [
                    {
                        "x-apikey": []
                    }
                ]
            }
        },
        "/api/data/avalanche/validator/info": {
            "get": {
                "operationId": "DataController_getAvalanchePageInfo",
                "summary": "Frontend Page Info For The Avalanche Validator",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiResponseDtoWithModel"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Data"
                ]
            }
        }
    },
    "info": {
        "title": "FAsset Indexer Api",
        "description": "Api for the FAsset indexer",
        "version": "1.0",
        "contact": {}
    },
    "tags": [],
    "servers": [],
    "components": {
        "securitySchemes": {
            "x-apikey": {
                "type": "apiKey",
                "in": "header",
                "name": "x-apikey"
            }
        },
        "schemas": {
            "FormDto": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "email",
                    "message"
                ]
            },
            "ApiResponseDto": {
                "type": "object",
                "properties": {
                    "status": {
                        "type": "number"
                    },
                    "data": {
                        "type": "object"
                    },
                    "error": {
                        "type": "string"
                    }
                },
                "required": [
                    "status"
                ]
            },
            "AvalancheInfoDto": {
                "type": "object",
                "properties": {
                    "validatorNodeId": {
                        "type": "string"
                    },
                    "validatorTransactionHash": {
                        "type": "string"
                    },
                    "apy": {
                        "type": "number"
                    },
                    "risk": {
                        "type": "string"
                    },
                    "validatorFee": {
                        "type": "number"
                    },
                    "validatorOwnedStake": {
                        "type": "string"
                    },
                    "validatorTotalStake": {
                        "type": "string"
                    },
                    "validatorAvailableCapacity": {
                        "type": "string"
                    },
                    "validatorStartTime": {
                        "type": "number"
                    },
                    "validatorEndTime": {
                        "type": "number"
                    },
                    "totalDelegators": {
                        "type": "number"
                    },
                    "totalDelegated": {
                        "type": "number"
                    },
                    "validatorUptime": {
                        "type": "number"
                    }
                },
                "required": [
                    "validatorNodeId",
                    "validatorTransactionHash",
                    "apy",
                    "risk",
                    "validatorFee",
                    "validatorOwnedStake",
                    "validatorTotalStake",
                    "validatorAvailableCapacity",
                    "validatorStartTime",
                    "validatorEndTime",
                    "totalDelegators",
                    "totalDelegated",
                    "validatorUptime"
                ]
            },
            "ApiResponseDtoWithModel": {
                "type": "object",
                "properties": {
                    "status": {
                        "type": "number"
                    },
                    "data": {
                        "$ref": "#/components/schemas/AvalancheInfoDto"
                    },
                    "error": {
                        "type": "string"
                    }
                },
                "required": [
                    "status"
                ]
            }
        }
    }
}